{
  "name": "examples.shop.support",
  "version": "0.1.0",
  "description": "the support package",
  "scripts": {
    "prepare": "orbital -b",
    "preinstall": "orbital -b",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Shim Heungwoon <hw.shim@navercorp.com>",
  "maintainers": [
    "Shim Heungwoon <hw.shim@navercorp.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "examples.shop.layout": "file:../examples.shop.layout"
  },
  "orbital": {
    "target": "web",
    "bundle": true,
    "contributable": {
      "services": [],
      "extensions": []
    },
    "contributes": {
      "services": [],
      "extensions": [{
        "id": "examples.shop.layout:contents",
        "realize": "./src/extensions/contents.js",
        "meta": {
          "path": "support"
        }
      }, {
        "id": "examples.shop.layout:aside",
        "realize": "./src/extensions/aside.js"
      }]
    }
