{
  "name": "examples.shop.resources",
  "version": "0.1.0",
  "description": "This package serves resources(Rest API, Images) for shop app",
  "scripts": {
    "prepare": "orbital -b",
    "preinstall": "orbital -b",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Shim Heungwoon <hw.shim@navercorp.com>",
  "maintainers": [
    "Shim Heungwoon <hw.shim@navercorp.com>"
  ],
  "license": "MIT",
  "dependencies": {
  },
  "orbital": {
    "target": "web",
    "bundle": true,
    "contributable": {
      "services": [
        {
          "id": "examples.shop.resources:api",
          "desc": "This spec provides rest api for shop",
          "spec": {
            "deleteFromCart": "function",
            "getAccount": "function",
            "getCart": "function",
            "getProductById": "function",
            "getProductCategories": "function",
            "getProducts": "function",
            "postToCart": "function",
            "postLogin": "function",
            "postLogout": "function"
          }
        },
        {
          "id": "examples.shop.resources:banners",
          "desc": "Provides rolling banners",
          "spec": {
            "getBanner": "function"
          },
          "async": true
        }
      ],
      "extensions": []
    },
    "contributes": {
      "services": [
        {
          "id": "examples.shop.resources:api",
          "realize": "./src/services/RestApi.js"
        },
        {
          "id": "examples.shop.resources:banners",
          "realize": "./src/services/RollingBanner.js"
        }
      ],
      "extensions": []
    }
  }
}
